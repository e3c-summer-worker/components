{
  "name": "p5-typescript-starter",
  "version": "1.0.0",
  "description": "Project to quickly get something working in [p5.js](https://p5js.org/) and [typescript](https://www.typescriptlang.org/)",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p start-compile start-run",
    "start-compile": "webpack --watch",
    "start-run": "browser-sync start --server -w",
    "build": "uglifyjs build/sketch.js build/PolygonHelper.js build/ColorHelper.js -o build/main-bundle.js -c -m"
  },
  "homepage": "https://github.com/Gaweph/p5-typescript-starter#readme",
  "devDependencies": {
    "@types/p5": "^1.3.0",
    "browser-sync": "^2.26.12",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.2.3",
    "typescript": "^3.9.10",
    "uglify-js": "^3.13.10",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "p5": "^1.4.0"
  }
}
