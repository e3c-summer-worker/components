{
  "name": "@e3c-summer-worker/homepage-ticker",
  "version": "1.0.0",
  "files": [
    "public/homepage-ticker-elm.js"
  ],
  "scripts": {
    "start:elm": "elm-live src/HomepageTicker.elm -H -p 8000 --dir=./public -- --output=public/homepage-ticker-elm.js",
    "start:scss": "node-sass src/ticker.scss -o public -w",
    "build": "run-p build:elm build:scss",
    "build:scss": "node-sass src/ticker.scss -o public",
    "build:elm": "npm run build:preprocess && npm run uglify && rm buffer.js",
    "build:preprocess": "elm make src/HomepageTicker.elm --optimize --output=buffer.js",
    "uglify": "uglifyjs buffer.js --compress 'pure_funcs=[F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9],pure_getters,keep_fargs=false,unsafe_comps,unsafe' | uglifyjs --mangle --output public/homepage-ticker-elm.js"
  },
  "devDependencies": {
    "elm-format": "^0.8.5",
    "elm-live": "^4.0.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.14.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
