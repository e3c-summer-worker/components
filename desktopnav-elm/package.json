{
    "scripts": {
        "start": "elm-live src/DesktopNav.elm -H --dir=./public -- --output=public/desktopnav-elm.js",
        "build": "npm run build:preprocess && npm run uglify && rm buffer.js",
        "build:preprocess": "elm make src/DesktopNav.elm --optimize --output=buffer.js",
        "uglify": "uglifyjs buffer.js --compress 'pure_funcs=[F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9],pure_getters,keep_fargs=false,unsafe_comps,unsafe' | uglifyjs --mangle --output public/desktopnav-elm.js"
    },
    "devDependencies": {
        "elm-live": "^4.0.2",
        "uglify-js": "^3.13.9"
    }
}
